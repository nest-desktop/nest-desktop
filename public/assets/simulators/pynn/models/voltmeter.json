{
  "abbreviation": "vm",
  "elementType": "recorder",
  "id": "voltmeter",
  "label": "voltmeter",
  "params": [
    {
      "id": "interval",
      "input": "tickSlider",
      "label": "time interval of recording",
      "rules": [
        [
          "value >= 0.1",
          "Small values generate many data points and can put quite a load on your browser.",
          "warning"
        ]
      ],
      "ticks": [0.01, 0.1, 1, 10],
      "unit": "ms",
      "value": 1
    },
    {
      "id": "start",
      "input": "valueSlider",
      "label": "start time",
      "max": 10000,
      "min": 0,
      "step": 10,
      "unit": "ms",
      "value": 0
    },
    {
      "id": "stop",
      "input": "valueSlider",
      "label": "stop time",
      "max": 10000,
      "min": 0,
      "step": 10,
      "unit": "ms",
      "value": 10000
    }
  ]
}
