    "d3": "^7.3",
    "debug": "^4.3",
    "detect-browser": "^5.3",
    "mathjs": "^10.0",
    "object-hash": "^2.2",
    "plotly.js-dist-min": "^2.9",
    "pouchdb": "^7.2",
    "pouchdb-upsert": "^2.2",
    "register-service-worker": "^1.7",
    "roboto-fontface": "*",
    "stats.js": "^0.17",
    "three": "^0.137",
    "uuid": "^8.3",
    "vue": "^2.6",
    "vue-class-component": "^7.2",
    "vue-codemirror": "^4.0",
    "vue-property-decorator": "9.1",
    "vue-router": "^3.5",
    "vue-toast-notification": "^0.6",
    "vuedraggable": "^2.24",
    "vuetify": "^2.6",
    "vuex": "^3.6"
  },
