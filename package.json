{
  "name": "nest-desktop",
  "version": "3.1.0",
  "private": false,
  "description": "NEST Desktop is a web-based application which provides a graphical user interface for NEST Simulator.",
  "scripts": {
    "serve": "vue-cli-service serve --port 8001",
    "build": "vue-cli-service build --prod",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e --env firefox --headless",
    "lint": "vue-cli-service lint",
    "docs:serve": "python3 -m http.server --directory ./docs/_build/html",
    "docs:build": "cd docs; make clean; make html",
    "electron:build": "vue-cli-service electron:build --publish never",
    "electron:serve": "vue-cli-service electron:serve",
    "electron:test": "vue-cli-service test:unit tests/unit/electron.spec.js",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps"
  },
  "main": "background.js",
  "dependencies": {
    "@mdi/font": "^6.5",
    "@vue/composition-api": "^1.4",
    "axios": "^0.26",
    "codemirror": "^5.65",
